            </Constraint>
            <Constraint ID="IZ-22">
                <Description>The value of OBX.11 (Observation Result Status) SHALL be 'F'.</Description>
                <Assertion>
                    <PlainText Path="11[1]" Text="F" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="SG[OBX]_CS_1">
