                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="d56ef43c-3d54-459b-880a-ad8aca357b2c">
            <Constraint ID="IZ-28">
                <Description>The value of RXA.1 (Give Sub-ID Counter) SHALL be '0'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="0" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-29">
                <Description>The value of RXA.2 (Administration Sub-ID Counter) SHALL be '1'.</Description>
                <Assertion>
                    <PlainText Path="2[1]" Text="1" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="SG[RXA]_CS_4">
                <Description>The value of RXA.3.1 (Time) SHALL be formatted with YYYYMMDD.</Description>
                <Assertion>
                    <Format Path="3[1].1[1]" Regex="([0-9]{4})((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1])).*"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-30">
                <Description>RXA.4.1 (Time) SHALL be identical to the RXA.3.1 (Time).</Description>
                <Assertion>
                    <PathValue Path1="4[1].1[1]" Operator="EQ" Path2="3[1].1[1]"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-48">
                <Description>If the value of RXA.20 (Completion Status) is 'RE', then the value of RXA.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <OR>
                        <NOT>
                            <PlainText Path="20[1]" Text="RE" IgnoreCase="true"/>
                        </NOT>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </OR>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ_50">
                <Description>If the value of RXA.9[1].1 (Identifier) is not '00', then the value of RXA.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <OR>
                        <NOT>
                            <NOT>
                                <PlainText Path="9[1].1[1]" Text="00" IgnoreCase="true"/>
                            </NOT>
                        </NOT>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </OR>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-49">
                <Description>If the value of RXA.1 (Give Sub-ID Counter) is '998', then the value of RXA.6 (Administered Amount) SHALL be '999'.</Description>
                <Assertion>
                    <OR>
                        <NOT>
                            <PlainText Path="1[1]" Text="998" IgnoreCase="true"/>
                        </NOT>
                        <PlainText Path="6[1]" Text="999" IgnoreCase="true"/>
                    </OR>
                </Assertion>
            </Constraint>
            <Constraint ID="SG[RXA]_CS_5">
                <Description>The value of RXA.16.1 (Time) SHALL be formatted with YYYYMM.</Description>
                <Assertion>
                    <Format Path="16[1].1[1]" Regex="YYYYMM"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-32">
                <Description>If RXA.18[1] (Substance/Treatment Refusal Reason) is valued, then the value of RXA.20 (Completion Status) SHALL be 'RE'.</Description>
                <Assertion>
                    <OR>
                        <NOT>
                            <Presence Path="18[1]"/>
                        </NOT>
                        <PlainText Path="20[1]" Text="RE" IgnoreCase="true"/>
                    </OR>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="5bca1cba-a959-4cba-9a66-7d8b471e867b">
            <Constraint ID="IZ-21">
