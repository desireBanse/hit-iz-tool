<?xml version="1.0" encoding="UTF-8"?>
<ConformanceContext UUID="81ec3838-6319-4cd6-ad90-0f4aef3a1938" xsi:noNamespaceSchemaLocation="ConformanceContext.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <MetaData>
        <Description>Conformance context for ACK</Description>
    </MetaData>
    <Datatype>
        <ByID ID="5d62ecfe-7a05-4d34-b034-361171a92f2f">
            <Constraint ID="IZ-5">
                <Description>The value of HD.2 (Universal ID) SHALL be formatted with ISO-compliant OID.</Description>
                <Assertion>
                    <Format Path="2[1]" Regex="[0-2](\.(0|[1-9][0-9]*))*"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-6">
                <Description>If HD.2 (Universal ID) is valued, then the value of HD.3 (Universal ID Type) SHALL be 'ISO'.</Description>
                <Assertion>
                    <OR>
                        <NOT>
                            <Presence Path="2[1]"/>
                        </NOT>
                        <PlainText Path="3[1]" Text="ISO" IgnoreCase="true"/>
                    </OR>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="48550434-594f-4ccc-8c80-c4144e3d17df">
            <Constraint ID="IZ-51">
                <Description>The value of MSG.1 (Message Code) SHALL be 'ACK'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="ACK" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-51">
                <Description>The value of MSG.2 (Trigger Event) SHALL be 'V04'.</Description>
                <Assertion>
                    <PlainText Path="2[1]" Text="V04" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-51">
                <Description>The value of MSG.3 (Message Structure) SHALL be 'ACK'.</Description>
                <Assertion>
                    <PlainText Path="3[1]" Text="ACK" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="15fba479-611e-4c04-b52d-27bd3a081559">
            <Constraint ID="IZ-7">
                <Description>The value of VID.1 (Version ID) SHALL be '2.5.1'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="2.5.1" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
        </ByID>
        <ByID ID="7017c48b-da5a-45e2-b4a1-2da6a6f19f2f">
            <Constraint ID="IZ-54">
                <Description>The value of EI.1 (Entity Identifier) SHALL be 'Z23'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="Z23" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-54">
                <Description>The value of EI.2 (Namespace ID) SHALL be 'CDCPHINVS'.</Description>
                <Assertion>
                    <PlainText Path="2[1]" Text="CDCPHINVS" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-3">
                <Description>The value of EI.3 (Universal ID) SHALL be formatted with ISO-compliant OID.</Description>
                <Assertion>
                    <Format Path="3[1]" Regex="[0-2](\.(0|[1-9][0-9]*))*"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-4">
                <Description>The value of EI.5.4 (Universal ID Type) SHALL be 'ISO'.</Description>
                <Assertion>
                    <PlainText Path="4[1]" Text="ISO" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
        </ByID>
    </Datatype>
    <Segment>
        <ByID ID="3bf4af76-c6f5-4114-baa7-7147064c325e">
            <Constraint ID="IZ-12">
                <Description>The value of MSH.1 (Field Separator) SHALL be '|'.</Description>
                <Assertion>
                    <PlainText Path="1[1]" Text="|" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-13">
                <Description>The value of MSH.2 (Encoding Characters) SHALL be '^~\&amp;'.</Description>
                <Assertion>
                    <PlainText Path="2[1]" Text="^~\&amp;" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-XX1">
                <Description>The value of MSH.7.1 (Time) SHALL be formatted with YYYYMMDD.</Description>
                <Assertion>
                    <Format Path="7[1].1[1]" Regex="([0-9]{4})((0[1-9])|(1[0-2]))((0[1-9])|([1-2][0-9])|(3[0-1])).*"/>
                </Assertion>
            </Constraint>
            <Constraint ID="IZ-53">
                <Description>The value of MSH.16 (Application Acknowledgment Type) SHALL be 'NE'.</Description>
                <Assertion>
                    <PlainText Path="16[1]" Text="NE" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
            <Constraint ID="SG[MSH]_CS_4">
                <Description>The value of MSH.16 (Application Acknowledgment Type) SHALL be 'NE'.</Description>
                <Assertion>
                    <PlainText Path="16[1]" Text="NE" IgnoreCase="true"/>
                </Assertion>
            </Constraint>
        </ByID>
    </Segment>
    <Group/>
</ConformanceContext>
